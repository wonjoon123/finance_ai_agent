ticker_name = "노루페인트"
start_date = "2024-06-01"
end_date = "2025-06-30"
short_window = 20
long_window = 60

date_cols = [c for c in stock_data.columns if c not in ['Price', 'Ticker', '시장', '종목명']]
close_data = stock_data[(stock_data['Price'] == 'Close') & (stock_data['종목명'] == ticker_name)].set_index('Ticker')
close_only = close_data[date_cols].astype(float)

ma_short = close_only.T.rolling(window=short_window).mean().T
ma_long = close_only.T.rolling(window=long_window).mean().T

signal = (ma_short > ma_long).astype(int)
signal_shift = signal.shift(axis=1)
dead_cross = (signal == 0) & (signal_shift == 1)
golden_cross = (signal == 1) & (signal_shift == 0)

date_range = pd.date_range(start=start_date, end=end_date).strftime('%Y-%m-%d')
valid_dates = [d for d in date_range if d in dead_cross.columns or d in golden_cross.columns]
dc_count = dead_cross[valid_dates].sum(axis=1).iloc[0]
gc_count = golden_cross[valid_dates].sum(axis=1).iloc[0]

total = dc_count + gc_count
print(f"{total}번")
